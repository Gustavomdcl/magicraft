<body class="no-js">

	<%- include('header'); -%>

	<div id="site">
		<div class="container row">
			<form class="col6" action="/play/" method="post">
				<div class="field">
					<label for="email">Email:</label>
					<input autocomplete="off" type="email" name="email" placeholder="Email">
				</div><!-- .field -->
				<div class="field">
					<label for="password">Senha:</label>
					<input autocomplete="off" type="password" name="password" placeholder="Senha">
				</div><!-- .field -->
				<button type="submit">Acessar</button>
			</form>
			<form class="col6" action="/register/" method="post">
				<div class="field">
					<label for="user">Apelido:</label>
					<input autocomplete="off" type="user" name="user" placeholder="Apelido">
				</div><!-- .field -->
				<div class="field">
					<label for="email">Email:</label>
					<input autocomplete="off" type="email" name="email" placeholder="Email">
				</div><!-- .field -->
				<div class="field">
					<label for="password">Senha:</label>
					<input autocomplete="off" type="password" name="password" placeholder="Senha">
				</div><!-- .field -->
				<button type="submit">Cadastrar</button>
			</form>
		</div><!-- .container -->
	</div><!-- #site -->

	<%- include('footer'); -%>

	<!-- javascript
	======================================================== -->
	<!-- jquery jquery.com -->
	<script src="backend/min/jquery.min.js?v=1.11.0"></script>
	<!-- suiting -->
