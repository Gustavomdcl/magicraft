<!DOCTYPE html>
<html lang="pt_BR">
<head>

	<meta charset="utf-8">

	<title>MagiCraft</title>

	<!-- description http://conversion.com.br rel="nofollow" on links
	======================================================== -->
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
	<meta name="robots" content="NOINDEX, nofollow" />	
	<meta name="title" content="MagiCraft">
	<meta name="description" content="MagiCraft">
	<meta name="author" content="Pinpinhos">
	<meta name="HandheldFriendly" content="True">
	<meta name="MobileOptimized" content="320">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta http-equiv="cleartype" content="on">

	<!-- styles
	======================================================== -->
	<!-- Main Style -->
	<link rel="stylesheet" href="backend/css/main.css">
	<!-- Adjust Style -->
	<link rel="stylesheet" href="backend/css/adjust.css">
	<!-- Font FONTE | font-family: 'FONTE', sans-serif; 
	<link href='http://google.com/fonts' rel='stylesheet' type='text/css'> -->
	<!-- modernizr modernizr.com -->
	<script src="backend/min/modernizr.min.js"></script>

</head>

<body class="no-js">

	<div id="site">
		<p>
			<% users.forEach(function(user,i){ %>
				<span><%= user %> (<%= i %>)</span>
			<% }); %>
		</p>
		<div class="container row">
			<form class="col6" action="/play/" method="post">
				<label for="email">Email:</label>
				<input autocomplete="off" type="email" name="email" placeholder="Email"><br>

				<label for="password">Senha:</label>
				<input autocomplete="off" type="password" name="password" placeholder="Senha"><br>

				<button type="submit">Acessar</button>
			</form>
			<form class="col6" action="/register/" method="post">
				<label for="to">To:</label>
				<input type="email" name="to">

				<label for="subject">Subject:</label>
				<input type="text" name="subject">

				<label for="body">Body:</label>
				<textarea cols="5" rows="5" name="body"></textarea>

				<button type="submit" class="btn btn-success">Send</button>
			</form>
		</div><!-- .container -->
	</div><!-- #site -->

	<!-- javascript
	======================================================== -->
	<!-- jquery jquery.com -->
	<script src="backend/min/jquery.min.js?v=1.11.0"></script>
	<!-- suiting -->
	<script src="backend/min/jquery.suiting.min.js"></script><!-- GRUNT http://blog.henriquesilverio.com/javascript-e-jquery/grunt-js-automatize-tarefas-e-otimize-o-seu-workflow/ -->
	<!-- mobile -->
	<script src="backend/min/jquery.mobile.min.js"></script><!-- GRUNT http://blog.henriquesilverio.com/javascript-e-jquery/grunt-js-automatize-tarefas-e-otimize-o-seu-workflow/ -->
	<!-- main -->
	<script src="backend/min/jquery.main.min.js"></script><!-- GRUNT http://blog.henriquesilverio.com/javascript-e-jquery/grunt-js-automatize-tarefas-e-otimize-o-seu-workflow/ -->

</body>
</html>